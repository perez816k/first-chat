<% provide(:title, 'Sign up') %>
<h1>Chat</h1>

<div class="center row">
  <div class="col-md-6 col-md-offset-3">
     <%= form_for(:session, url: login_path) do |f| %>
     
      <%= f.text_field :name, placeholder: "Name" %>

      <%= f.submit "Enter", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>