<% @messages.each do |m| %>
    <li>
        <%= m.content %>
    </li>
<% end %>

<%= form_for Message.new do |f| %>
    <%= f.text_field :content, placeholder: "Write..."  %>
    <%= f.hidden_field :user, :value => session[:user_name] %>
    <%= f.hidden_field :room_id, :value => 1 %>
    <%= f.submit "Send", class: "btn btn-primary" %>
<% end %>